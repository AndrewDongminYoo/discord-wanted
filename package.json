{
  "name": "discord-wanted",
  "private": true,
  "version": "1.0.0",
  "description": "Discord example app",
  "main": "app.js",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "npx tsx app.js",
    "register": "npx tsx commands.js"
  },
  "author": "Dongmin Yu",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "discord-interactions": "^3.4.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "serverless": "^4.4.5",
    "serverless-offline": "^14.3.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "overrides": {
    "serverless-offline": {
      "serverless": "$serverless"
    }
  }
}
